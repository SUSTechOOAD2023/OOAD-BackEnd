<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.RelationshipStudentClassGroupMapper">
    <select id="selectByCompositeKey" resultType="com.example.entity.RelationshipStudentClassGroup">
        SELECT * FROM relationship_student_class_group
        WHERE student_id = #{studentId} AND group_id = #{groupId}
    </select>

</mapper>
